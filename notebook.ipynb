      "naive plan: (run LazyFrame.explain(optimized=True) to see the optimized plan)\n",
      "\n",
      " WITH_COLUMNS:\n",
      " [col(\"photo_ids\").fill_null([[]]), col(\"photo_count\").fill_null([0])] \n",
      "  SELECT [col(\"business_id\"), col(\"exact_stars\").alias(\"stars\"), col(\"review_count\"), col(\"photo_count\"), col(\"photo_ids\")]\n",
      "    INNER JOIN:\n",
      "    LEFT PLAN ON: [col(\"business_id\")]\n",
      "      INNER JOIN:\n",
      "      LEFT PLAN ON: [col(\"business_id\")]\n",
      "        FILTER col(\"categories\").str.contains([\"Restaurants\"])\n",
      "        FROM\n",
      "          NDJson SCAN [data/dataset-json/yelp_academic_dataset_business.json] [id: 4452287008]\n",
      "          PROJECT */14 COLUMNS\n",
      "      RIGHT PLAN ON: [col(\"business_id\")]\n",
      "        AGGREGATE[maintain_order: false]\n",
      "          [col(\"photo_id\").alias(\"photo_ids\"), col(\"label\").alias(\"photo_labels\"), len().alias(\"photo_count\")] BY [col(\"business_id\")]\n",
      "          FROM\n",
      "          NDJson SCAN [data/dataset-photos/photos.json] [id: 4452286912]\n",
      "          PROJECT */4 COLUMNS\n",
      "      END INNER JOIN\n",
      "    RIGHT PLAN ON: [col(\"business_id\")]\n",
      "      AGGREGATE[maintain_order: false]\n",
      "        [col(\"stars\").mean().alias(\"exact_stars\")] BY [col(\"business_id\")]\n",
      "        FROM\n",
      "        SELECT [col(\"business_id\"), col(\"stars\")]\n",
      "          NDJson SCAN [data/dataset-json/yelp_academic_dataset_review.json] [id: 4452287040]\n",
      "          PROJECT */9 COLUMNS\n",
      "    END INNER JOIN\n"
     ]
